@{
    ViewBag.Title = "Home Page";
}

<!DOCTYPE html>
 
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Events</title>
</head>
<body>
<div>
    <h3>List events</h3>
    <table class="table">
        <tr><td><p>NameEvent</p></td>
            <td><p>Address</p></td>
            <td><p>Description</p></td>
            <td><p></p></td>
        </tr>
        @foreach (var b in ViewBag.Events)
        {
            <tr>
                <td><p>@b.NameEvent</p></td>
                <td><p>@b.Address</p></td>
                <td><p>@b.Description</p></td>
                <td><p><a href="/Home/Event/@b.Id">More info</a></p></td>
            </tr>
        }
    </table>
</div>
</body>
</html>